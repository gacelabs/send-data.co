{
    "name": "silverstripe/installer",
    "type": "silverstripe-recipe",
    "description": "The SilverStripe Framework Installer",
    "require": {
        "php": ">=7.1.0",
        "silverstripe/recipe-plugin": "^1.2",
        "silverstripe/recipe-cms": "4.5.1@stable",
        "silverstripe-themes/simple": "~3.2.0",
        "undefinedoffset/sortablegridfield": "^2.0",
        "symbiote/silverstripe-gridfieldextensions": "^3.2",
        "colymba/gridfield-bulk-editing-tools": "^3.0",
        "silverstripe/display-logic": "^2.0",
        "silverstripe/blog": "^3.5",
        "ryanpotter/silverstripe-color-field": "^1.0",
        "wilr/silverstripe-googlesitemaps": "^2.1",
        "axllent/silverstripe-minifier": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "extra": {
        "resources-dir": "_resources",
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600,
        "secure-http": false
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
